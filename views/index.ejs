<html>
    <head>
        <title><%= title %></title>
    </head>

    <body>
        <ul>
            <h1><%= title %></h1>
            <button><a href="/new">New Message</a></button>
            <% for( let i = 0; i < messages.length; i++ ) { %>
            <li>
                <h3><%= messages[i].user %></h3>
                <p><%= messages[i].text %></p>
                <a href="/message/<%= i %>"><button>Open</button></a>
            </li>
            <% } %>
        </ul>
        
    </body>
</html>